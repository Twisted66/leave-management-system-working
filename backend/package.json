{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "npm@10.5.0",
  "scripts": {
    "build": "npm run prebuild && cd ../frontend && rm -rf node_modules && rm -rf dist && rm -rf .vite && npm install --no-cache && npm run build && cp -r dist/* ../backend/frontend/dist/",
    "clean": "rm -rf frontend/dist && rm -rf static",
    "prebuild": "npm run clean && mkdir -p frontend/dist"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "encore.dev": "^1.49.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}